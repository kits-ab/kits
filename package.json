{
  "name": "@kokitotsos/kits",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kits-ab/kits.git"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf .cache public",
    "format": "prettier \"src/**/*.{js,json,jsx,md,ts,tsx}\" --write",
    "generateschema": "gql-gen",
    "lint": "yarn lint:css && yarn lint:ts",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint:ts": "tslint -p tsconfig.json -c tslint.prod.json",
    "precommit": "lint-staged",
    "prepush": "yarn lint",
    "start": "gatsby develop --host 0.0.0.0",
    "test": "yarn lint"
  },
  "dependencies": {
    "@kokitotsos/react-components": "1.3.3",
    "babel-plugin-styled-components": "1.10.6",
    "date-fns": "2.1.0",
    "dotenv": "8.1.0",
    "gatsby": "2.15.14",
    "gatsby-link": "2.2.13",
    "gatsby-plugin-catch-links": "2.1.9",
    "gatsby-plugin-feed": "2.3.12",
    "gatsby-plugin-manifest": "2.2.16",
    "gatsby-plugin-netlify": "2.1.12",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-netlify-cms": "4.1.14",
    "gatsby-plugin-offline": "3.0.6",
    "gatsby-plugin-react-helmet": "3.1.7",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.2.21",
    "gatsby-plugin-sitemap": "2.2.12",
    "gatsby-plugin-styled-components": "3.1.5",
    "gatsby-plugin-typescript": "2.1.7",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.21",
    "gatsby-remark-prismjs": "3.3.13",
    "gatsby-remark-smartypants": "2.1.8",
    "gatsby-source-filesystem": "2.1.22",
    "gatsby-transformer-remark": "2.6.22",
    "gatsby-transformer-sharp": "2.1.14",
    "gatsby-transformer-yaml": "2.2.9",
    "mapbox-gl": "0.54.1",
    "netlify-cms": "2.9.7",
    "normalize.css": "8.0.1",
    "polished": "3.4.1",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-mapbox-gl": "4.6.0",
    "react-transition-group": "4.3.0",
    "remark": "11.0.1",
    "remark-html": "10.0.0",
    "slugify": "1.3.5",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.10",
    "@types/styled-components": "4.1.19",
    "graphql": "14.5.4",
    "graphql-code-generator": "0.18.2",
    "graphql-codegen-typescript-client": "0.18.2",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-codegen-typescript-server": "0.18.2",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.6.3",
    "uglifyjs-webpack-plugin": "2.2.0"
  },
  "resolutions": {
    "sharp": "0.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "pre-push": "yarn prepush"
    }
  },
  "lint-staged": {
    "*.{js,json,jsx,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components"
    ],
    "rules": {
      "declaration-colon-newline-after": null,
      "no-descending-specificity": null,
      "value-list-max-empty-lines": null
    },
    "syntax": "scss"
  }
}
