{
  "compilerOptions": {
    "baseUrl": "src",
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "stripInternal": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015"
  },
  "include": ["definitions/**/*", "src/**/*"]
}
