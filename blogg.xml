---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ 'KITS - Blogg' | xml_escape }}</title>
    <description>{{ 'Senaste inl√§ggen i KITS blogg' | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/blogg.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.categories.blogg %}
    <item>
      <title>{{ post.heading | xml_escape }}</title>
      <link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <author>{% for authorId in post.authors %}{% assign author = site.medarbetare | concat: site.medarbetare_old | where: "uid", authorId | first %}{% unless forloop.first %}, {% endunless %}{{ author.name }}{% endfor %}</author>
      <guid isPermaLink="true">{{ post.url | prepend: site.baseurl | prepend: site.url }}</guid>
      <description>{{ post.content | replace: '<!--more-->','' | markdownify | replace: '<h6>','<div class="c-image-grid">' | replace: '</h6>','</div>' | xml_escape }}</description>
      <category>{{ post.categories | last | xml_escape }}</category>
    </item>
    {% endfor %}
  </channel>
</rss>
